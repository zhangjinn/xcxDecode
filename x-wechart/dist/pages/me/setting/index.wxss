.ellipsis {
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow-wrap: break-word;
  overflow: hidden;
}
.setting .setting-list {
  border-radius: 0 0 16rpx 16rpx;
  overflow: hidden;
}
.setting .setting-list .setting-custom {
  padding: 30rpx 24rpx;
}
.setting .setting-list .setting-custom .van-cell__title {
  font-size: 32rpx;
}
.setting .setting-list .setting-custom .van-cell__right-icon-wrap {
  color: #AAAAAA;
}
.setting .setting-list .setting-custom-last::after {
  border-bottom-width: 0;
}
.setting-logout {
  margin-top: 24rpx;
}
.setting-logout .logout-btn {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  height: 96rpx;
  font-size: 32rpx;
  background: #FFFFFF;
  border-radius: 16rpx;
}
.setting .wechat-popup {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  padding: 0 36rpx;
  height: 752rpx;
}
.setting .wechat-popup-actions {
  width: 580rpx;
}
.setting .wechat-popup-actions-title {
  font-size: 38rpx;
  font-weight: 500;
  color: #262626;
  margin-bottom: 140rpx;
}
