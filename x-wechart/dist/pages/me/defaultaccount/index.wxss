.ellipsis {
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow-wrap: break-word;
  overflow: hidden;
}
.me-default-account {
  padding: 16rpx;
}
.me-default-account .account-item {
  background-color: #fff;
  padding: 16rpx;
  margin-bottom: 16rpx;
}
.me-default-account .account-item-header {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.me-default-account .account-item-header-tag {
  background: #dfdfdf;
  color: #00AAAF;
  font-size: 9px;
  line-height: 9px;
  height: 9px;
  margin: 10rpx;
}
.me-default-account .account-item-footer {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  color: #dfdfdf;
}
.me-default-account .account-item-footer-primary {
  font-size: 14px;
}
.me-default-account .account-item-footer-info {
  font-size: 9px;
}
