.ellipsis {
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow-wrap: break-word;
  overflow: hidden;
}
.me-account-list {
  height: 100%;
}
.me-account-list .order-scroll {
  height: 100%;
}
.me-account-list .me-account-admin {
  margin-top: 16rpx;
  border-bottom: 2rpx solid #EEEEEE;
}
.me-account-list .account-item {
  padding: 24rpx;
  background: #FFF;
}
.me-account-list .account-item:last-child {
  margin-bottom: 0;
}
.me-account-list .account-item .account-header {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  margin-bottom: 16rpx;
}
.me-account-list .account-item .account-header .account-type {
  font-size: 26rpx;
  color: #262626;
  line-height: 36rpx;
  height: 36rpx;
}
.me-account-list .account-item .account-header .account-type .businessFlagName {
  font-size: 24rpx;
  margin-left: 12rpx;
  color: #00AAA6;
  background-color: #ECF8F8;
}
.me-account-list .account-item .account-header .account-status {
  background: #00AAA6;
  color: #FFF;
  font-size: 20rpx;
  line-height: 36rpx;
  height: 36rpx;
  border-radius: 18rpx;
  padding: 2rpx 16rpx;
}
.me-account-list .account-item .account-header .account-status.disable {
  background: #D9161C;
}
.me-account-list .account-item-options {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: end;
  -webkit-justify-content: flex-end;
          justify-content: flex-end;
  background: #FFF;
  padding: 24rpx;
  border-top: 2rpx solid #EEEEEE;
  margin-bottom: 16rpx;
}
.me-account-list .account-item-options van-button {
  margin-left: 16rpx;
}
.me-account-list .account-auth-system-list {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  margin-bottom: 16rpx;
}
.me-account-list .account-auth-system-list .account-auth-system-item {
  background: #ECF8F8;
  color: #00AAA6;
  font-size: 20rpx;
  padding: 2rpx 8rpx;
  margin-right: 8rpx;
}
.me-account-list .account-info-line {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  margin-bottom: 12rpx;
  font-size: 24rpx;
  line-height: 32rpx;
}
.me-account-list .account-info-line .info-label {
  color: #777777;
  height: 32rpx;
}
.tip-popup .tip-modal {
  width: 604rpx;
}
.tip-popup .tip-modal .tip-label {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  font-size: 36rpx;
  min-height: 300rpx;
  padding: 24rpx;
}
.tip-popup .tip-modal .tip-options {
  font-size: 32rpx;
  color: #00AAA6;
  padding: 32rpx 270rpx;
  border-top: 2rpx solid #EEEEEE;
  background-color: #EEEEEE;
}
.update-password-popup {
  width: 100%;
  height: 100%;
}
.update-password-popup .form-footer {
  margin-top: 20rpx;
  padding: 40rpx;
}
