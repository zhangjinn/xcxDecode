<van-count-down use-slot time="{{ time }}" bind:change="onChange">
  <view wx:if="{{timeData.days !== 0 }}" class="days">{{ timeData.days }}å¤©</view>
  <view class="items">{{ timeData.hours }}</view>:
  <view class="items">{{ timeData.minutes }}</view>:
  <view class="items">{{ timeData.seconds }}</view>
</van-count-down>
