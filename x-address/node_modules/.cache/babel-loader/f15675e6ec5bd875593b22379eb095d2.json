  }\n  })\n\n  let sheetName = filename //excel的文件名称\n  let wb = XLSX.utils.book_new()  //工作簿对象包含一SheetNames数组，以及一个表对象映射表名称到表对象。XLSX.utils.book_new实用函数创建一个新的工作簿对象。\n  let ws = XLSX.utils.json_to_sheet(newData, { header: Object.values(fields) }) //将JS对象数组转换为工作表。\n  wb.SheetNames.push(sheetName)\n  wb.Sheets[sheetName] = ws\n  const defaultCellStyle = { font: { name: \"Verdana\", sz: 13, color: \"FF00FF88\" }, fill: { fgColor: { rgb: \"FFFFAA00\" } } };//设置表格的样式\n  let wopts = { bookType: 'xlsx', bookSST: false, type: 'binary', cellStyles: true, defaultCellStyle: defaultCellStyle, showGridLines: false }  //写入的样式\n  let wbout = XLSX.write(wb, wopts)\n  let blob = new Blob([s2ab(wbout)], { type: 'application/octet-stream' })\n  fs.saveAs(blob, filename + '.xlsx')\n}\nfunction s2ab(s) {\n  if (typeof ArrayBuffer !== 'undefined') {\n    let buf = new ArrayBuffer(s.length)\n    let view = new Uint8Array(buf)\n    for (let i = 0; i != s.length; ++i) view[i] = s.charCodeAt(i) & 0xff\n    return buf\n  } else {\n    let buf = new Array(s.length);\n    for (let i = 0; i != s.length; ++i) buf[i] = s.charCodeAt(i) & 0xFF;\n    return buf;\n  }\n}\nexport { downloadXlsxFile };\n"],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAAA;AACA,OAAOA,EAAP,MAAe,YAAf;AACA,OAAO,KAAKC,IAAZ,MAAsB,eAAtB;;AAEA,SAASC,gBAAT,CAA0BC,SAA1B,EAAqCC,MAArC,EAA6CC,QAA7C,EAAuD;EACrD;AACF;AACA;AACA;AACA;EAEE,IAAIC,IAAI,GAAGC,IAAI,CAACC,SAAL,CAAeL,SAAf,CAAX;EACA,IAAIM,OAAO,GAAGF,IAAI,CAACG,KAAL,CAAWJ,IAAX,CAAd;EAEAG,OAAO,CAACE,OAAR,CAAgB,UAACC,IAAD,EAAU;IACxB,IAAIA,IAAJ,EAAU;MACR,KAAK,IAAIC,CAAT,IAAcD,IAAd,EAAoB;QAClB,IAAIE,MAAM,GAAGP,IAAI,CAACG,KAAL,CAAWH,IAAI,CAACC,SAAL,CAAeI,IAAI,CAACC,CAAD,CAAnB,CAAX,CAAb;QACA,OAAOD,IAAI,CAACC,CAAD,CAAX,CAFkB,CAEF;;QAChB,IAAIT,MAAM,CAACW,cAAP,CAAsBF,CAAtB,CAAJ,EAA8B;UAC5BD,IAAI,CAACR,MAAM,CAACS,CAAD,CAAP,CAAJ,GAAkBC,MAAlB;QACD;MACF;IACF;EACF,CAVD;EAYA,IAAIE,SAAS,GAAGX,QAAhB,CAtBqD,CAsB5B;;EACzB,IAAIY,EAAE,GAAGhB,IAAI,CAACiB,KAAL,CAAWC,QAAX,EAAT,CAvBqD,CAuBrB;;EAChC,IAAIC,EAA